              "**/node_modules/**",
              "!apps/mreza-mladih-platform/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mreza-mladih-platform"],
          "options": {
            "jestConfig": "apps/mreza-mladih-platform/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
